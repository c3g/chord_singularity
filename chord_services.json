[
  {
    "id": "service_registry",
    "repository": "https://bitbucket.org/genap/chord_service_registry",
    "data_service": false,
    "apt_dependencies": [],
    "python_module": "chord_service_registry.app",
    "python_callable": "application",
    "python_environment": {
      "CHORD_SERVICES": "/chord/chord_services.json",
      "DATABASE": "{SERVICE_DATA}/service_registry.db"
    }
  },
  {
    "id": "rnaget",
    "repository": "https://github.com/CanDIG/rnaget_service",
    "data_service": true,
    "apt_dependencies": [],
    "python_module": "candig_rnaget.app",
    "python_callable": "application",
    "python_args": [
      "--database", "{SERVICE_DATA}/rnaget.db",
      "--logfile", "{SERVICE_LOGS}/rnaget.log",
      "--tmpdata", "{SERVICE_TEMP}/"
    ]
  }
]
